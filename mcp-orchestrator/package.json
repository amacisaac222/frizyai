{
  "name": "@frizy/mcp-orchestrator",
  "version": "0.1.0",
  "description": "Frizy MCP Orchestrator - connects Claude Code to Frizy project management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts api",
    "dev:mcp": "tsx watch src/index.ts mcp",
    "dev:consumer": "tsx watch src/consumer.ts",
    "dev:graphql": "tsx watch src/graphql-standalone.ts",
    "dev:embeddings": "tsx watch src/embedding-processor.ts",
    "start": "node dist/index.js api",
    "start:mcp": "node dist/index.js mcp",
    "start:api": "node dist/index.js api",
    "start:consumer": "node dist/consumer.js",
    "start:graphql": "node dist/graphql-standalone.js",
    "start:embeddings": "node dist/embedding-processor.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:integration": "tsx src/integration-test.ts",
    "migrate": "tsx src/migrate.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "openai": "^4.67.3",
    "pg": "^8.12.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "keywords": [
    "mcp",
    "claude",
    "project-management",
    "ai",
    "orchestrator"
  ],
  "author": "Frizy Team",
  "license": "MIT"
}
